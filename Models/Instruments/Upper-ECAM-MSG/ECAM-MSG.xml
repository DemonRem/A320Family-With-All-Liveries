<PropertyList>

	<path>EMPTY.ac</path>
	
	<model>
		<path>LEFT-MSG.xml</path>
		<condition>
			<or>
				<equals>
					<property>ECAM/left-msg</property>
					<value>MSG</value>
				</equals>
				<equals>
					<property>ECAM/left-msg</property>
					<value>TO-MEMO</value>
				</equals>
			</or>
		</condition>
	</model>
	
	<model>
		<path>TO-MEMO.xml</path>
		<condition>
			<equals>
				<property>ECAM/left-msg</property>
				<value>TO-MEMO</value>
			</equals>
		</condition>
	</model>
	
</PropertyList>
