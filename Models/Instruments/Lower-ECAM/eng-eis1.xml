	<!-- Colors -->
	
	<!-- Green -->
	<animation>
		<type>material</type>
		<object-name>txt.qty1</object-name>
		<object-name>txt.qty2</object-name>
		<object-name>txt.psi1</object-name>
		<object-name>txt.psi2</object-name>
		<diffuse>
			<red>0</red>
			<green>0.543</green>
			<blue>0</blue>
		</diffuse>
		<ambient>
			<red>0</red>
			<green>0</green>
			<blue>0</blue>
		</ambient>
		<specular>
			<red>0</red>
			<green>0</green>
			<blue>0</blue>
		</specular>
		<emission>
			<red>0</red>
			<green>0.543</green>
			<blue>0</blue>
		</emission>
	</animation>
	
	<!-- Red -->
	<animation>
		<type>material</type>
		<object-name>txt.psi1r</object-name>
