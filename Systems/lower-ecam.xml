					<property>/engines/engine[0]/oil-psi-actual</property>
					<entry><ind>  0</ind><dep>-90</dep></entry>
					<entry><ind> 50</ind><dep>  0</dep></entry>
					<entry><ind>100</ind><dep> 90</dep></entry>
